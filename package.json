{
  "name": "ai-mock-interview",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "db:push": "npx drizzle-kit push"
  },
  "eslintConfig": {
    "extends": [
      "wesbos"
    ],
    "rules": {
      "no-console": 0,
      "prettier/prettier": [
        "error",
        {
          "singleQuote": true,
          "endOfLine": "auto",
          "tabWidth": 2,
          "trailingComma": "none"
        }
      ],
      "no-plusplus": "off",
      "import/no-unresolved": 0,
      "react/prop-types": 0,
      "import/no-extraneous-dependencies": 0,
      "no-undef": 0
    }
  },
  "dependencies": {
    "@clerk/nextjs": "^5.2.8",
    "@radix-ui/react-slot": "^1.1.0",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.1",
    "eslint-config-wesbos": "^4.0.1",
    "lucide-react": "^0.408.0",
    "next": "14.2.5",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "tailwind-merge": "^2.4.0",
    "tailwindcss-animate": "^1.0.7"
  },
  "devDependencies": {
    "postcss": "^8",
    "tailwindcss": "^3.4.1"
  }
}
