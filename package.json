{
  "name": "ai-mock-interview",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "db:push": "npx drizzle-kit push"
  },
  "eslintConfig": {
    "extends": [
      "wesbos"
    ],
    "rules": {
      "no-console": 0,
      "prettier/prettier": [
        "error",
        {
          "singleQuote": true,
          "endOfLine": "auto",
          "tabWidth": 2,
          "trailingComma": "none"
        }
      ],
      "no-plusplus": "off",
      "import/no-unresolved": 0,
      "react/prop-types": 0,
      "import/no-extraneous-dependencies": 0,
      "no-undef": 0,
      "jsx-a11y/click-events-have-key-events": 0,
      "jsx-a11y/no-static-element-interactions": 0,
      "no-unsafe-optional-chaining": 1
    }
  },
  "dependencies": {
    "@clerk/nextjs": "^5.2.8",
    "@google/generative-ai": "^0.16.0",
    "@neondatabase/serverless": "^0.9.4",
    "@radix-ui/react-dialog": "^1.1.1",
    "@radix-ui/react-label": "^2.1.0",
    "@radix-ui/react-slot": "^1.1.0",
    "axios": "^1.7.3",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.1",
    "date-fns": "^3.6.0",
    "drizzle-orm": "^0.33.0",
    "eslint-config-wesbos": "^4.0.1",
    "lucide-react": "^0.408.0",
    "next": "14.2.5",
    "next-themes": "^0.3.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-hook-speech-to-text": "^0.8.0",
    "react-webcam": "^7.2.0",
    "short-unique-id": "^5.2.0",
    "sonner": "^1.5.0",
    "tailwind-merge": "^2.4.0",
    "tailwindcss-animate": "^1.0.7"
  },
  "devDependencies": {
    "drizzle-kit": "^0.24.0",
    "postcss": "^8",
    "tailwindcss": "^3.4.1"
  }
}
